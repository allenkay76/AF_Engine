{
    "tasks": [
        {
            "label": "clean",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/C",
                "del",
                "/F",
                "/Q",
                "${workspaceFolder}\\bin\\AF_Engine\\*"
            ],
            "problemMatcher": [],
            "group": "build"
        },
        {
            "type": "shell",
            "label": "C/C++: g++.exe build active file",
            "command": "C:\\MinGW\\bin\\g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-I${workspaceFolder}/include",
                "-Wall",
                "-Wextra",
                "-Werror",
                "-pedantic",
                "-Wconversion",
                "-Wformat-security",
                "-Wnull-dereference",
                "-Wdouble-promotion",
                "-Wshadow",
                "-fstack-protector-strong",
                "-D_FORTIFY_SOURCE=2",
                "${workspaceFolder}/src/*",
                "-o",
                "${workspaceFolder}\\bin\\AF_Engine.exe"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger.",
            "dependsOn": "clean"
        },
        {
            "type": "shell",
            "label": "C/C++: cl.exe build Windows Debug",
            "command": "cl.exe",
            "args": [
                "/nologo",
                "/EHsc",
                "/I${workspaceFolder}/include",
                "/IC:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\Community\\VC\\Tools\\MSVC\\14.29.30133\\include",
                "/IC:\\Program Files (x86)\\Windows Kits\\10\\Include\\10.0.19041.0\\ucrt",
                "/MDd",
                "/W4",
                "/WX",
                "/permissive-",
                "/std:c++17",
                "/sdl",
                "/D_CRT_SECURE_NO_WARNINGS",
                "/Fo:${workspaceFolder}\\bin\\",
                "/Fe:${workspaceFolder}\\bin\\AF_Engine.exe",
                "${workspaceFolder}/src/*.cpp"
            ],
            "options": {
                "cwd": "${workspaceFolder}",
                "env": {
                    "LIB": "C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\Community\\VC\\Tools\\MSVC\\14.29.30133\\lib\\x64;C:\\Program Files (x86)\\Windows Kits\\10\\Lib\\10.0.19041.0\\ucrt\\x64;C:\\Program Files (x86)\\Windows Kits\\10\\Lib\\10.0.19041.0\\um\\x64"
                }
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
            "kind": "build",
            "isDefault":  true
            },
            "detail": "Task generated by Debugger.",
            "dependsOn": "clean"
        }
    ],
    "version": "2.0.0"
}
